{
  "name": "urban-map",
  "version": "0.0.1",
  "description": "Site do Mapa de Acessibilidade Urbana",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "NODE_ENV=development nodemon ./src/server.js",
    "build": "next build src",
    "prestart": "yarn migration:up && yarn build",
    "start": "NODE_ENV=production node ./src/server.js",
    "lint": "eslint --cache 'src/**/*.{js,jsx}'",
    "migration": "node -r dotenv/config ./node_modules/.bin/sequelize",
    "migration:create": "npm run migration migration:generate --",
    "migration:create:model": "npm run migration model:generate --",
    "migration:create:seed": "npm run migration seed:generate --",
    "migration:up": "npm run migration db:migrate",
    "migration:down": "npm run migration db:migrate:undo",
    "migration:seed": "npm run migration db:seed:all",
    "migration:unseed": "npm run migration db:seed:undo"
  },
  "author": "Lucas Alves de Sousa <lucas_comp@outlook.com>",
  "license": "UNLICENSED",
  "homepage": "https://github.com/lucascomp/urban-map#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucascomp/urban-map.git"
  },
  "bugs": {
    "url": "https://github.com/lucascomp/urban-map/issues"
  },
  "dependencies": {
    "@react-google-maps/api": "^1.7.6",
    "@zeit/next-css": "^1.0.1",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.2.1",
    "kcors": "^2.2.2",
    "koa": "^2.8.1",
    "koa-bodyparser": "^4.2.1",
    "koa-connect": "^2.1.0",
    "koa-generic-session": "^2.0.4",
    "koa-logger": "^3.2.1",
    "koa-passport": "^4.1.3",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "next": "^9.5.4",
    "next-cookies": "^2.0.3",
    "next-images": "^1.4.1",
    "next-pwa": "^3.1.1",
    "nodemailer": "^6.4.11",
    "nookies": "^2.0.8",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-local": "^1.0.0",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-modal": "^3.11.1",
    "react-outside-click-handler": "^1.3.0",
    "sequelize": "^5.21.1",
    "sequelize-cli": "^5.5.1",
    "uuid": "^3.3.3",
    "validator": "^12.0.0"
  },
  "devDependencies": {
    "eslint": "6.1.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "nodemon": "^2.0.1"
  }
}
